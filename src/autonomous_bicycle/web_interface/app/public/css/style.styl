raiseText()
  color grey
  text-shadow -1px 1px 0 white
  -moz-text-shadow -1px 1px 0 white
  -webkit-text-shadow -1px 1px 0 white

body
  margin 0
  padding 0
  height 100%
  background rgb(0, 0, 0)

h2 {
  margin: 0 0 6px 0;
  font-weight: bold;
}

h6 {
  margin: 2px 0 20px 0;
}

hr {
  margin 14px 0
  border-top: 1px solid rgb(238, 238, 238);
  border-bottom: 1px solid rgb(255, 255, 255);
}

ul {
  margin: 4px 0 0 0;
}

label, h6 {
  color: #808080;
  font-size: 20px;
  font-weight: normal;
  text-shadow: -1px 1px 0 #fff;
}

a{
  font-size: 25px;
  }
.well {
  /*background: rgba(85, 85, 85, 0.8);*/
  .tab-title {
    float: left;
    margin-left: 10px;
  }
  .button-top-right {
    float: right;
    margin: 0 10px 20px 0;
  }
  margin-bottom 0;
  height 90%;
  top 55%;
}
  .navbar-title{
    font-size: 30px;
  }

.navbar-btn {
  min-width: 100px
  margin-right: 8px;
  .btn {
    min-width: 100px
  }
}


.glyphicon {
  margin-right: 8px;
}

/*
	login window
*/

#login {
  label {
    margin-left: 2px;
  }
  form {
    width: 320px;
  }
  .controls {
    margin-top: 20px;
    .button-rememember-me {
      float: left;
      width: 150px;
    }
    .button-sign-in {
      float: right;
      width: 120px;
    }
  }
  .btm-links {
    font-size: .9em;
    margin 2px 0 0 0
    #forgot-password {
      float left
      margin-left 10px;
    }
    #create-account {
      float right
      margin-right 5px;
    }
  }
}

#account-form-container {
  form {
    width: 460px;
  }
}

/*
	modal windows
*/

.modal {
  .modal-dialog {
    width: 480px;
  }
  .form-group {
    margin-bottom: 0px;
  }
  .alert {
    margin-top 10px;
    margin-bottom: 0
    padding 8px 15px 8px 15px
  }
  .alert-dismissable, .close {
    right -4px;
  }
  p {
    margin: 0;
    padding: 0;
  }
  .modal-footer {
    button {
      width: 80px;
    }
  }
}

.center-vertical {
  height: 100vh;
  form {
    top: 50%;
    margin: auto;
    position: relative;
    -webkit-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    transform: translateY(-50%);
  }
}

.form-horizontal {
  .form-buttons {
    float right;
    .btn {
      width 80px
      margin-left 6px
    }
  }
  input, select {
    height 30px
  }
}

.row_style {
  text-align center;
  align-content text-align center;
  width 100%;
}

.row_style_fixed {
  text-align center;
  align-content text-align center;
  width 50px;
}

.btn_control_up_down {
  text-align center;
  align-content text-align center;
  width 60%;
  margin 5px;
  height 50px;
  z-index 9999;
  position absolute;
}

.btn_control_left_right {
  text-align center;
  align-content text-align center;
  height 160px;
  z-index 9999;
  position absolute;
}

.div_viewer_style {
  position: absolute;
  display: block;
  z-index 100;
  height: 95%;
  width: 97%;
}

.img_style {
  width: 85%;
  max-height: 480px;
  max-width: 640px;
  display: block;
  margin-left: auto;
  margin-right: auto
}

.div_progress_bar {
  width 200px;
  z-index 9999;
  display inline-block;
  overflow inherit;
}


		
